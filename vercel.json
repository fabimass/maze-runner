{
    "builds": [
      {
        "src": "maze_project/wsgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles_build"
        }
      }
    ],
    "routes": [
      { 
        "src": "/static/(.*)", 
        "dest": "static/$1" 
      },
      {
        "src": "/(.*)",
        "dest": "maze_project/wsgi.py"
      }
    ]
}